}

QPushButton
{
    color: #e1e1e1;
    background-color: #5d5d5d;
    border-width: 1px;
    border-color: #606060;
    border-style: solid;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
    padding-right: 5px;
    border-radius: 1px;
    outline: none;
}

QPushButton:disabled
{
    color: #454545;
    background-color: #555555;
    border-width: 1px;
    border-color: #404040;
    border-style: solid;
    padding-top: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
    padding-right: 5px;
    border-radius: 1px;
    outline: none;
}

QPushButton:focus
{
    background-color: #5d5d5d;
}

QPushButton:hover
{
    background-color: #47a9b6;
    border: 1px solid #47a9b6;
    color: #101010;
}

QPushButton:checked
{
    background-color: #202020;
    border-color: #303030;
    color: #c1c1c1;
}

QPushButton:pressed
{
    background-color: #252525;
    color: #c1c1c1;
    border: none;
}

QToolButton
{
    color: #e1e1e1;
    background-color: #5d5d5d;
    border-width: 1px;
    border-color: #606060;
    border-style: solid;
    padding-top: 4px;
    padding-bottom: 3px;
    padding-left: 5px;
    padding-right: 15px;
    border-radius: 1px;
    outline: none;
}

QToolButton:focus
{
    background-color: #5d5d5d;
}

QToolButton:hover
{
    background-color: #47a9b6;
    border: 1px solid #47a9b6;
    color: #101010;
}

QToolButton:checked
{
    background-color: #47a9b6;
    border-color: #47a9b6;
    color: #101010;
}

QToolButton:pressed
{
    background-color: #47a9b6;
    border-color: 47a9b6;
    color: #101010;
}

QToolButton::menu-indicator
{
    top: -3px; left: -2px;
}

QToolButton::menu-indicator:pressed
{
    top: -5px; left: -4px;
}

QTabWidget
{
    border: 1px transparent black;
}

QTabWidget:focus
{
    border: none;
}

QTabWidget::pane
{
    border: 1px solid #292929;
    border-radius: 3px;
    padding: 1px;
}

QTabBar::close-button
{
    background: transparent;
}

QTabBar::tab:top
{
    border-left: 1px solid #444444;
    border-bottom: 1px transparent black;
    background-color: #47a9b6;
    padding-top: 5px;
    padding-bottom: 3px;
    padding-left: 15px;
    padding-right: 15px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    color: #eeeeee;
}

QTabBar::tab:top:!selected
{
    background-color: #353535;
}

QTabBar::tab:top:!selected:hover
{
    background-color: #47a9b6;
    color: #101010;
}

QComboBox
