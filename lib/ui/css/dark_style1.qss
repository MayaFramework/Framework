QWidget
{
    color: #9BAAA9;
    background-color: #282b2e;
    selection-background-color:#627f93;
    selection-color: black;
    background-clip: border;
    border-image: none;
    outline: 0;
      font-family:"Arial Unicode MS";
}


QWidget:focus
{
    border: 1px solid #47a9b6;
}

QWidget:disabled
{
    color: #404040;
    background-color: #444444;
}

QWidget:item:hover
{
    background-color: #c1c1c1;
    color: #202020;
}

QWidget:item:selected
{
    background-color: #606060;
}

QToolTip
{
    border: 1px solid #3A3939;
    background-color: rgb(90, 102, 117);
    color: white;
    padding: 1px;
    opacity: 200;
}

QGroupBox
{
    border:1px solid #292929;
    border-radius: 2px;
    margin-top: 20px;
}

QGroupBox::title
{
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 10px;
    padding-left: 0px;
    padding-right: 0px;
    padding-top: 13px;
}

QGroupBox::indicator
{
    width: 15;
    height: 15;
    margin-left: 2px;
}

QCheckBox
{
    spacing: 5px;
    outline: none;
    color: #c5c5c5;
    margin-bottom: 2px;
}

QCheckBox:focus
{
    border: none;
}

QCheckBox:disabled
{
    color: #777777;
}

QCheckBox::indicator
{
    width: 15;
    height: 15;
}

QRadioButton
{
    spacing: 5px;
